&spmi_bus {
	qcom,pmiplutonium@2 {
		spmi-slave-container;
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0x2>;

		pmiplutonium_revid: qcom,revid@100 {
			compatible = "qcom,qpnp-revid";
			reg = <0x100 0x100>;
		};
	};

	qcom,pmplutonium@3 {
		spmi-slave-container;
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0x3>;
	};
};
